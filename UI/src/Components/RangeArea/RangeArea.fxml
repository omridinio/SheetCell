<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rangeAreaController" alignment="CENTER" maxHeight="1.0E17" maxWidth="1.0E19" prefHeight="401.0" prefWidth="286.0" stylesheets="@RangeArea.CSS" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Components.RangeArea.RangeAreaController">
   <children>
      <HBox id="buttomsRange" alignment="CENTER" prefHeight="73.0" prefWidth="286.0" spacing="30.0">
         <children>
            <Button fx:id="addRange" mnemonicParsing="false" onAction="#addRangeClicked" text="Add Range" />
            <Button fx:id="deleteRange" disable="true" mnemonicParsing="false" onAction="#deleteRangeClicked" text="Delete Range" />
         </children>
      </HBox>
      <ScrollPane>
         <content>
            <FlowPane id="rangeArea" fx:id="rangeArea" focusTraversable="true" hgap="15.0" prefHeight="222.0" prefWidth="251.0" vgap="10.0">
               <padding>
                  <Insets left="5.0" top="5.0" />
               </padding>
            </FlowPane>
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" prefHeight="49.0" prefWidth="281.0" spacing="15.0">
         <children>
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelClicked" prefHeight="21.0" prefWidth="73.0" text="Cancel" />
            <Button fx:id="sumbitDelete" mnemonicParsing="false" onAction="#deleteSelctedRange" prefHeight="27.0" prefWidth="138.0" text="Sumbit Delete Range" />
         </children>
      </HBox>
   </children>
</VBox>
