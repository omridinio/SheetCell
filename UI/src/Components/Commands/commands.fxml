<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="299.0" prefWidth="255.0" spacing="2.0" stylesheets="@commands.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Components.Commands.CommandsController">
   <children>
      <HBox prefHeight="90.0" prefWidth="205.0" spacing="2.0">
         <children>
            <Button fx:id="sort" mnemonicParsing="false" onAction="#clickeSort" prefHeight="90.0" prefWidth="90.0" text="Sort" />
            <Button fx:id="filter" mnemonicParsing="false" onAction="#filterClicked" prefHeight="90.0" prefWidth="90.0" text="Filter" />
         </children>
      </HBox>
      <HBox prefHeight="90.0" prefWidth="205.0" spacing="2.0">
         <children>
            <Button fx:id="dynmicAnlyzeForCell" mnemonicParsing="false" onAction="#dynmicCellClicked" prefHeight="80.0" prefWidth="80.0" text="Dynmic Anlyze for Select Cell" textAlignment="CENTER" wrapText="true" />
            <Button mnemonicParsing="false" onAction="#dynmicSheetClicked" prefHeight="80.0" prefWidth="80.0" text="Dynmic Anlayze for Sheet" textAlignment="CENTER" wrapText="true" />
         </children>
      </HBox>
      <HBox prefHeight="90.0" prefWidth="205.0" spacing="2.0">
         <children>
            <Button mnemonicParsing="false" onAction="#createGraphClicked" prefHeight="80.0" prefWidth="80.0" text="Create a Graph" textAlignment="CENTER" wrapText="true" />
            <Button fx:id="customFormula" mnemonicParsing="false" onAction="#customFormulaClicked" prefHeight="80.0" prefWidth="80.0" text="Custom formula" textAlignment="CENTER" wrapText="true" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="50.0" top="8.0" />
   </padding>
</VBox>
